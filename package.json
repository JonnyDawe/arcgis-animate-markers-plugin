{
  "name": "arcgis-animate-markers-plugin",
  "description": "A plugin to animate markers in ArcGIS.",
  "keywords": [
    "arcgis",
    "animate",
    "markers",
    "plugin"
  ],
  "private": false,
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonnyDawe/arcgis-animate-markers-plugin.git"
  },
  "module": "./dist/arcgis-animate-markers-plugin.mjs",
  "exports": {
    ".": {
      "import": "./dist/arcgis-animate-markers-plugin.mjs",
      "module": "./dist/arcgis-animate-markers-plugin.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/arcgis-animate-markers-plugin.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "LICENSE",
    "src/*"
  ],
  "scripts": {
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "format:scripts": "prettier . --write",
    "format": "npm run format:scripts"
  },
  "peerDependencies": {
    "@arcgis/core": "^4.26"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.17.3",
    "@types/react": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-c8": "^0.29.8",
    "dts-bundle-generator": "^7.2.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.4.8",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "mini-svg-data-uri": "^1.4.4",
    "wobble": "^1.5.1"
  },
  "license": "MIT"
}
