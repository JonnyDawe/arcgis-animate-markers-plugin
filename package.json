{
  "name": "arcgis-animate-markers-plugin",
  "description": "A plugin to animate markers in ArcGIS.",
  "keywords": [
    "arcgis",
    "animate",
    "markers",
    "plugin"
  ],
  "type": "module",
  "private": false,
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonnyDawe/arcgis-animate-markers-plugin.git"
  },
  "module": "./dist/arcgis-animate-markers-plugin.mjs",
  "exports": {
    ".": {
      "import": "./dist/arcgis-animate-markers-plugin.mjs",
      "module": "./dist/arcgis-animate-markers-plugin.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/arcgis-animate-markers-plugin.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "LICENSE",
    "src/*"
  ],
  "scripts": {
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled=true",
    "test:coverage": "vitest run --coverage",
    "lint": "npx eslint --fix --config eslint.config.mjs './src/**/*.{js,jsx,ts,tsx}' './tests/**/*.{js,jsx,ts,tsx}'",
    "check-types": "tsc --noEmit -p tsconfig.json",
    "format:scripts": "prettier . --write",
    "format": "npm run format:scripts"
  },
  "peerDependencies": {
    "@arcgis/core": "^4.26"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.17.3",
    "@types/react": "^18.2.18",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^8.0.3",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.13.1",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "mini-svg-data-uri": "^1.4.4",
    "wobble": "^1.5.1"
  },
  "license": "MIT",
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.18.0"
  }
}
